#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#time
xsd:time rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/fabiz/ontologies/2023/9/untitled-ontology-49/hasA
:hasA rdf:type owl:ObjectProperty ;
      rdfs:domain :Race ;
      rdfs:range [ rdf:type owl:Class ;
                   owl:unionOf ( :Qualifying
                                 :Sprint
                               )
                 ] .


###  http://www.semanticweb.org/fabiz/ontologies/2023/9/untitled-ontology-49/hasCircuit
:hasCircuit rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Race ;
            rdfs:range :Circuit .


###  http://www.semanticweb.org/fabiz/ontologies/2023/9/untitled-ontology-49/hasConstructor
:hasConstructor rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Partecipation ;
                rdfs:range :Constructor .


###  http://www.semanticweb.org/fabiz/ontologies/2023/9/untitled-ontology-49/hasCountry
:hasCountry rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Location ;
            rdfs:range <http://sweet.jpl.nasa.gov/2.3/humanJurisdiction.owl#Country> .


###  http://www.semanticweb.org/fabiz/ontologies/2023/9/untitled-ontology-49/hasDriver
:hasDriver rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Partecipation ;
           rdfs:range :Driver .


